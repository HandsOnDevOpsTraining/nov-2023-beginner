    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
                echo 'Everyone'
                sh 'mkdir madan'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Test'
            }
        }
    }
}
